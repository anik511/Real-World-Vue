/address>\n\n    <h2>Event details</h2>\n    <p>{{ event.description }}</p>\n\n    <h2>\n      Attendees\n      <span class=\"badge -fill-gradient\">{{\n        event.attendees ? event.attendees.length : 0\n      }}</span>\n    </h2>\n    <ul class=\"list-group\">\n      <li\n        v-for=\"(attendee, index) in event.attendees\"\n        :key=\"index\"\n        class=\"list-item\"\n      >\n        <b>{{ attendee.name }}</b>\n      </li>\n    </ul>\n  </div>\n</template>\n\n<script>\nimport EventService from \"@/services/EventService\";\nexport default {\n  props: [\"id\"],\n  data() {\n    return {\n      event: {},\n    };\n  },\n  created() {\n    EventService.getEvent(this.id)\n      .then((response) => {\n        console.log(response.data);\n        this.event = response.data;\n      })\n      .catch((error) => {\n        console.log(\"Axios Error in EventShow: \", error.response);\n      });\n  },\n};\n</script>\n\n<style scoped>\n.location {\n  margin-bottom: 0;\n}\n.location > .icon {\n  margin-left: 10px;\n}\n.event-header > .title {\n  margin: 0;\n}\n.list-group {\n  margin: 0;\n  padding: 0;\n  list-style: none;\n}\n.list-group > .list-item {\n  padding: 1em 0;\n  border-bottom: solid 1px #e5e5e5;\n}\n.badge.-fill-gradient {\n  padding-bottom: 27px !important;\n}\n</style>\n"],"sourceRoot":"src/views"}]}