;AAgBA,IAAA,sBAhBA,oCAgBA;AACA,UAAA,IAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,IAAA;AACA,UAAA,EAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,MAAA,KAAA,QAAA,CAAA;AACA,aAAA;AACA,QAAA,EAAA,EAAA,EADA;AAEA,QAAA,IAAA,EAAA,IAFA;AAGA,QAAA,QAAA,EAAA,EAHA;AAIA,QAAA,SAAA,EAAA,IAJA;AAKA,QAAA,KAAA,EAAA,EALA;AAMA,QAAA,WAAA,EAAA,EANA;AAOA,QAAA,QAAA,EAAA,EAPA;AAQA,QAAA,IAAA,EAAA,EARA;AASA,QAAA,IAAA,EAAA,EATA;AAUA,QAAA,SAAA,EAAA;AAVA,OAAA;AAYA;AA/BA,KAgCA,UAAA,CAAA,OAAA,EAAA,CAAA,aAAA,CAAA,CAhCA;AAhBA,CAAA","sourcesContent":["<template>\n  <div>\n    <h1>Create an Event</h1>\n    <form @submit.prevent=\"newEvent\">\n      <BaseSelect\n        label=\"Select a category\"\n        :options=\"categories\"\n        v-model=\"event.category\"\n        class=\"field\"\n      />\n\n      <h3>Name & describe your event</h3>\n      <BaseInput\n        label=\"Title\"\n        v-model=\"event.title\"\n        type=\"text\"\n        placeholder=\"Title\"\n        class=\"field\"\n      />\n      <BaseInput\n        label=\"Description\"\n        v-model=\"event.description\"\n        type=\"text\"\n        placeholder=\"Add a description\"\n        class=\"field\"\n      />\n\n      <h3>Where is your event?</h3>\n      <BaseInput\n        label=\"Location\"\n        v-model=\"event.location\"\n        type=\"text\"\n        placeholder=\"Add a location\"\n        class=\"field\"\n      />\n\n      <h3>When is your event?</h3>\n\n      <div class=\"field\">\n        <label>Date</label>\n        <datepicker v-model=\"event.date\" placeholder=\"Select a date\" />\n      </div>\n\n      <BaseSelect\n        label=\"Select a time\"\n        :options=\"times\"\n        v-model=\"event.time\"\n        class=\"field\"\n      />\n      <multiselect\n        label=\"Select a time\"\n        :options=\"times\"\n        v-model=\"event.time\"\n        class=\"field\"\n      ></multiselect>\n\n      <input type=\"submit\" class=\"button\" value=\"Submit\" />\n    </form>\n  </div>\n</template>\n\n<script>\nimport Multiselect from 'vue-multiselect'\nimport Datepicker from 'vuejs-datepicker'\nimport { mapActions } from 'vuex'\nimport NProgress from 'nprogress'\n\nexport default {\n  components: {\n    Datepicker,\n    Multiselect\n  },\n  data() {\n    const times = []\n    for (let i = 1; i <= 24; i++) {\n      times.push(i + ':00')\n    }\n    return {\n      times,\n      categories: this.$store.state.categories,\n      event: this.createFreshEventObject()\n    }\n  },\n  methods: {\n    newEvent() {\n      NProgress.start()\n      this.createEvent(this.event)\n        .then(() => {\n          this.createFreshEventObject()\n          this.$router.push({\n            name: 'event-show',\n            params: { id: this.event.id }\n          })\n        })\n        .catch(() => {\n          NProgress.done()\n          console.log('There was a problem creating event')\n        })\n    },\n    createFreshEventObject() {\n      const user = this.$store.state.user.user\n      const id = Math.floor(Math.random() * 10000000)\n      return {\n        id: id,\n        user: user,\n        category: '',\n        organizer: user,\n        title: '',\n        description: '',\n        location: '',\n        date: '',\n        time: '',\n        attendees: []\n      }\n    },\n    ...mapActions('event', ['createEvent'])\n  }\n}\n</script>\n\n<style scoped>\n.filed {\n  margin-bottom: 24px;\n}\n.button {\n  margin-top: 20px;\n  padding: 10px 20px;\n}\n</style>\n"],"sourceRoot":"src/views"}]}