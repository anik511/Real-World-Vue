ate.users.length < payload) {\n        console.log(\"Add user mode\");\n        state.user = null;\n        state.newid = payload;\n      } else if(state.users.length >= payload){\n        console.log(state.users.find(user => user.id == payload));\n        state.user = state.users.find(user => user.id == payload);\n        console.log(\"Edit user mode\");\n      }\n    },\n    deleteUser(state, payload){\n      console.log(state.users.filter(user => !(user.id == payload)));\n      state.users = state.users.filter(user => !(user.id == payload))\n    },\n    updateUser(state, payload){\n      console.log(\"Store:\",payload.id);\n      if (payload.id == 0) {\n        console.log(\"new user\");\n        payload.id = state.newid;\n        payload.active = true;\n        state.users.push(payload)\n      } else {\n        console.log(\"user updated\");\n        var index = state.users.findIndex(x => x.id == payload.id);\n        state.users[index] = payload;\n        // console.log(state.users);\n      }\n      \n    },\n    searchUsers(state, payload){\n\n      state.users = state.users.filter(user => user.user_name.toLowerCase().indexOf(payload.toLowerCase()) !== -1)\n    },\n  },\n  actions: {\n    setReviews(contex, payload) {\n      contex.commit('setReviews', payload)\n    },\n    setreviewdetais(contex, payload) {\n      contex.commit('setreviewdetais', payload)\n    },\n    loginInfo(contex, payload) {\n      contex.commit('loginInfo', payload)\n    },\n    setUsers(contex, payload){\n      contex.commit('setUsers',payload)\n    },\n    deleteUser(contex, payload){\n      contex.commit('deleteUser', payload)\n    },\n    editUser(contex, payload){\n      contex.commit('editUser', payload)\n    },\n    updateUser(contex, payload){\n      contex.commit('updateUser', payload)\n    },\n    searchUsers(contex, payload){\n      contex.commit('searchUsers', payload)\n    },\n  },\n  modules: {\n  }\n})\n"]}]}